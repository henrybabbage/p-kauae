{
    "plugins": [
        "prettier"
    ],
    "extends": [
        "next/core-web-vitals",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "import/no-anonymous-default-export": "off",
        "react-hooks/exhaustive-deps": "warn",
        "prettier/prettier": "error",
        "no-console": "warn",
        "react/no-children-prop": "off",
        "indent": [
            "warn",
            4
        ]
    }
}